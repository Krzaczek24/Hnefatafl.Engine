<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <PackageProjectUrl>https://github.com/Krzaczek24?tab=packages</PackageProjectUrl>
        <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
        <RepositoryUrl>https://github.com/Krzaczek24/KrzaqTools</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageId>Krzaq.Hnefatafl.Engine</PackageId>
        <Version>1.0.0</Version>
        <Authors>Tomasz 'Krzaczek' Drewek</Authors>
        <PackageReadmeFile>README.md</PackageReadmeFile>
    </PropertyGroup>

    <ItemGroup>
        <None Include="README.md" Pack="true" PackagePath="" />
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="Krzaq.Collections.ReadOnlyTable" Version="1.0.1" />
        <PackageReference Include="Krzaq.Extensions.IReadOnlyTable" Version="1.0.0" />
    </ItemGroup>

</Project>
